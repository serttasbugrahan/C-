//merge

int main()
{
using namespace std;

list<string> x;
list<string> y;

auto fgen = []{return rname() + ' ' + rfname(); };

rfill(x, 20, fgen);
rfill(y, 20, fgen);

x.sort();
y.sort();

x.merge(y); //sıralı birleştirme yapıyor

cout << "x.size() = "<< x.size() << '\n';
cout << "y.size() = "<< y.size() << '\n';
(void)getchar();

print(x, "\n");
}

---------------

int main()
{
using namespace std;

list<string> x;
list<string> y;

auto fgen = []{return rname() + ' ' + rfname(); };
auto fcomp = [](const string&s1, const string& s2) {
return s1.lenght() < s2.lenght() || (s1.lenght() == s2.lenght() && s1 < s2)
};

rfill(x, 20, fgen);
rfill(y, 20, fgen);

x.sort(fcomp);
y.sort(fcomp);

x.merge(y, fcomp);

cout << "x.size() = "<< x.size() << '\n';
cout << "y.size() = "<< y.size() << '\n';
(void)getchar();

print(x, "\n");
}

